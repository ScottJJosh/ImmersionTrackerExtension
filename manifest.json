{
    "manifest_version": 3,
    "name": "Subtitle Extractor",
    "description": "Extract subtitles from Animelon.",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://animelon.com/*"
    ]
  }
  